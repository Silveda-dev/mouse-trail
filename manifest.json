{
    "manifest_version": 3,

    "name": "mouse trail",

    "version": "0.1",

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["mouse-trail.js"]
        }
    ],

    "web_accessible_resources": [
        {
            "resources": ["skull_flag.png"],
            "matches": ["<all_urls>"]
        }
    ]
}
