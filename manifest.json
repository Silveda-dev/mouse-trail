{
    "manifest_version": 3,

    "name": "pride mouse",

    "version": "0.1",

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["pride-mouse.js"]
        }
    ],

    "web_accessible_resources": [
        {
            "resources": ["lesbian.jpg"],
            "matches": ["<all_urls>"]
        }
    ]
}